{
  "name": "@feng3d/objectview",
  "version": "0.3.4",
  "description": "ObjectView是feng3d中实现由数据对象自动生成界面的一套框架。",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bundle": "dist/index.js",
  "bundleName": "feng3d",
  "bundle:types": "dist/index.d.ts",
  "author": "feng",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf lib",
    "prebundle": "rimraf dist",
    "build": "tsc -b",
    "build:w": "tsc -b -w",
    "bundle": "rollup -c",
    "bundle:w": "rollup -c -w ",
    "lint": "eslint --ext .js --ext .ts src test rollup.config.js --ignore-path .gitignore --max-warnings 0",
    "lintfix": "npm run lint -- --fix",
    "test": "mocha",
    "docs": "rimraf public && typedoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/feng3d/objectview.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://gitlab.com/feng3d/objectview/issues"
  },
  "homepage": "https://gitlab.com/feng3d/objectview#readme",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "lib",
    "src",
    "readme"
  ],
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/mocha": "^8.2.3",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "mocha": "^9.0.2",
    "rimraf": "3.0.2",
    "rollup": "^2.32.1",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-typescript2": "^0.28.0",
    "ts-node": "^10.1.0",
    "typedoc": "^0.21.4",
    "typedoc-plugin-sourcefile-url": "^1.0.6",
    "typescript": "^4.4.3"
  }
}